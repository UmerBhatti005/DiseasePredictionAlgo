{
  "version": 2,
  "builds": [
    {
      "src": "api/model_training.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.0" }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/model_training.py",
      "methods": ["POST", "GET"]
    }
  ]
}